#:kivy 1.8.0-dev

#: import player players

<WarBackground>:
  pc: hp

  Player:
    id: hp
    center: 0, 0

<Player>:
  size: 15, 10
  canvas:
    Color:
      rgb: 0.3, 0.4, 0.1
    Ellipse:
      size: self.size
      pos: self.pos
    Color:
      rgb: 0.3, 0.2, 0.1
    Ellipse:
      size: 7, 6
      pos: self.center_x - 3.5, self.center_y - 3
