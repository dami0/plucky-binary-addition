#:kivy 1.8.0-dev

<WarBackground>:
  player: hp
  w_vert: vw

  WarPlayer:
    id: hp
    center: 700, 400

  Vertical_Wall:
    id: vw
    center: 1250, 50

<WarPlayer>:
  size: 10, 10
